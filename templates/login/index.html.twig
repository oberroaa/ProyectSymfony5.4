<section>
    <div class="container">
        <div class="row">

            <div class="col-md-6 col-md-offset-3">

                <!-- ALERT -->
                {% if error %}
                    <div class="alert alert-mini alert-danger margin-bottom-30">
                        <strong>Error!</strong> {{ error.messageKey|trans(error.messageData, 'security') }}
                    </div><!-- /ALERT -->
                {% endif %}

                <div class="box-static box-border-top padding-30">
                    <div class="box-title margin-bottom-30">
                        <h2 class="size-20">Login Usuario</h2>
                    </div>

                    <form class="nomargin" method="post" action="{{ path('login') }}" autocomplete="off">
                        <div class="clearfix">

                            <!-- Email -->
                            <div class="form-group">
                                <input type="text" id="username" name="_username" class="form-control" required="" placeholder="username" value="{{ last_username }}"/>
                            </div>
                            <!-- Password -->
                            <div class="form-group">
                                <input type="password" id="password" name="_password" class="form-control" placeholder="Password" required="">
                            </div>

                        </div>

                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <!-- Inform Tip -->
                                <div class="form-tip pt-20">
                                    <a class="no-text-decoration size-13 margin-top-10 block" href="#">Forgot Password?</a>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-6 text-right">
                                <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
                                <button class="btn btn-primary" type="submit">login</button>
                            </div>
                        </div>
                    </form>

                </div>

                <div class="margin-top-30 text-center">
                    <a href="#"><strong>Registrar Cuenta</strong></a>

                </div>
            </div>

        </div>
    </div>

</section>


